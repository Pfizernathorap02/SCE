<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="com.pfizer.sce.beans.EventCourse" table="EVENT_COURSE">
		<meta attribute="class-description">
			This class contains the eventcourse detail.
		</meta>
		<id name="eventId" column="EVENT_ID" type="int" />
		<property name="eventCourseId" column="EVENT_COURSE_ID" type="int" />		
		<property name="courseName" column="COURSE_NAME" type="string" />
		<property name="templateId" column="TEMPLATE_ID" type="int" />
		<property name="tableName" column="TABLE_NAME" type="string" />

	</class>
</hibernate-mapping>