<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="com.pfizer.sce.beans.TrainerLearnerMapping" table="TRAINER_LEARNERS_MAPPING">
		<meta attribute="class-description">
		This class contains employee detail.
		</meta>
		<id name="mapId" type="string" column="MAP_ID" />
		<property name="learnerName" column="LEARNER_NAME" type="string"
			unique="false"/>
		<property name="trainerName" column="TRAINER_NAME" type="string" 
			unique="false"/>
		<property name="learnerEmail" column="LEARNER_EMAIL" type="string"
			unique="false"/>
		<property name="trainerEmail" column="TRAINER_EMAIL" type="string"
			unique="false"/>
		<property name="product" column="PRODUCT" type="string"
			unique="false"/>
		<property name="eventName" column="EVENT_NAME" type="string"
			unique="false"/>
		<property name="learnerLoc" column="LEARNER_LOC" type="string"
			unique="false"/>
		<property name="trainerLoc" column="TRAINER_LOC" type="string"
			unique="false"/>
		<property name="courseStartDate" column="COURSE_START_DATE" type="date"
			unique="false"/>
		<property name="emailSent" column="EMAIL_SENT" type="string"
			unique="false"/>
	</class>
</hibernate-mapping>