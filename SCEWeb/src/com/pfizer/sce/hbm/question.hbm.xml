<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

	<class name="com.pfizer.sce.beans.Question" table="QUESTION">
		<meta attribute="class-description">
			This class contains the question detail.
		</meta>
		<id name="id" column="QUESTION_ID" type="int" />
		<property name="templateVersionId" column="template_version_id" type="int" />
		<property name="title" column="TITLE" type="string" />		
		<property name="displayOrder" column="DISPLAY_ORDER" type="int" />
		<property name="description" column="DESCRIPTION" type="string" />
		<property name="critical" column="CRITICAL" type="string" />
</class>
</hibernate-mapping>